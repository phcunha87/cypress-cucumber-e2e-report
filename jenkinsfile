pipeline{
    agent any
        stages {
            stage('Just Test') {
                steps {
                    bat 'npm run test'
                }
            }
            stage ('ALLURE REPORT') {
                 steps {
                    allure includeProperties: false, jdk: '', results: [[path: 'target/surefire-reports']]
                
                }
            }   
            

        }

}    

